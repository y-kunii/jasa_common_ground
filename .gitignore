# This .gitignore file should be placed at the root of your Unity project directory
#
# Get latest from https://github.com/github/gitignore/blob/main/Unity.gitignore
#
plugin-srcs/[Ll]ibrary/
plugin-srcs/[Tt]emp/
plugin-srcs/[Oo]bj/
plugin-srcs/[Bb]uild/
plugin-srcs/[Bb]uilds/
plugin-srcs/[Ll]ogs/
plugin-srcs/[Uu]ser[Ss]ettings/

# MemoryCaptures can get excessive in size.
# They also could contain extremely sensitive data
plugin-srcs/[Mm]emoryCaptures/

# Recordings can get excessive in size
plugin-srcs/[Rr]ecordings/

# Uncomment this line if you wish to ignore the asset store tools plugin
# /[Aa]ssets/AssetStoreTools*

# Autogenerated Jetbrains Rider plugin
plugin-srcs/[Aa]ssets/Plugins/Editor/JetBrains*

# Visual Studio cache directory
.vs/

# Gradle cache directory
.gradle/

# Autogenerated VS/MD/Consulo solution and project files
plugin-srcs/ExportedObj/
plugin-srcs/.consulo/
plugin-srcs/*.csproj
plugin-srcs/*.unityproj
plugin-srcs/*.sln
plugin-srcs/*.suo
plugin-srcs/*.tmp
plugin-srcs/*.user
plugin-srcs/*.userprefs
plugin-srcs/*.pidb
plugin-srcs/*.booproj
plugin-srcs/*.svd
plugin-srcs/*.pdb
plugin-srcs/*.mdb
plugin-srcs/*.opendb
plugin-srcs/*.VC.db

# Unity3D generated meta files
plugin-srcs/*.pidb.meta
plugin-srcs/*.pdb.meta
plugin-srcs/*.mdb.meta

# Unity3D generated file on crash reports
plugin-srcs/sysinfo.txt

# Builds
plugin-srcs/*.apk
plugin-srcs/*.aab
plugin-srcs/*.unitypackage
plugin-srcs/*.app

# Crashlytics generated file
plugin-srcs/crashlytics-build.properties

# Packed Addressables
plugin-srcs/[Aa]ssets/[Aa]ddressable[Aa]ssets[Dd]ata/*/*.bin*

# Temporary auto-generated Android Assets
plugin-srcs/[Aa]ssets/[Ss]treamingAssets/aa.meta
plugin-srcs/[Aa]ssets/[Ss]treamingAssets/aa/*
